<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ACTION BAR -->
    <declare-styleable name="lobi_ActionBar">
        <attr name="lobi_content_type">
            <enum name="lobi_manual" value="0" />
            <enum name="lobi_backable" value="1" />
            <enum name="lobi_select_account" value="2" />
            <enum name="lobi_title_only" value="3" />
            <enum name="lobi_close" value="4" />
            <enum name="lobi_menu" value="5" />
        </attr>
        <attr name="lobi_text" format="string" />
    </declare-styleable>

</resources>